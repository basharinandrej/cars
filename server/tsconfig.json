{
  "compilerOptions": {
    "target": "ES6",
    "sourceMap": true,
    "module": "commonjs",
    "baseUrl": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "paths": {
      "@models*": ["models/*"],
      "@routers*": ["routers/*"],
      "@common*": ["common/*"],
      "@db*": ["db/*"],
    }, 
  }
}
